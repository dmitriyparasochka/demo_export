  "version": "0.2.0",
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "demo_export:HelloWorldFunction",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/demo_export/template.yaml",
        "logicalId": "HelloWorldFunction"
      },
      "lambda": {
        "payload": {},
        "environmentVariables": {}
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "API demo_export:HelloWorldFunction",
      "invokeTarget": {
        "target": "api",
        "templatePath": "${workspaceFolder}/demo_export/template.yaml",
        "logicalId": "HelloWorldFunction"
      },
      "api": {
        "path": "/hello",
        "httpMethod": "get",
        "payload": {
          "json": {}
        }
      },
      "lambda": {
        "runtime": "python3.8"
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "demo_export:ApplicationResourceGroup",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/demo_export/template.yaml",
        "logicalId": "ApplicationResourceGroup"
      },
      "lambda": {
        "payload": {},
        "environmentVariables": {}
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "demo_export:ApplicationInsightsMonitoring",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/demo_export/template.yaml",
        "logicalId": "ApplicationInsightsMonitoring"
      },
      "lambda": {
        "payload": {},
        "environmentVariables": {}
      }
    }
  ]
}